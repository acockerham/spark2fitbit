<h1>Edit My Account</h1>

<% form_for @user, :url => account_path do |f| %>
  <%= f.error_messages %>
  <%= label_tag(:user_email,"Email: ") %><%= text_field_tag("user_email", @user.email) %>
  <!-- To print entire object  <%= render :partial => "form", :object => f %> -->
  <%= f.submit "Update" %>
<% end %>

<br /><%= link_to "My Profile", account_path %>
